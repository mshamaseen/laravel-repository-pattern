{
    "name": "shamaseen/repository-generator",
    "description": "repository pattern files generator",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Shamaseen",
            "email": "m.shamaseen@outlook.com"
        },
        {
            "name": "Hamza Alayed",
            "email": "developerh108@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Shamaseen\\Repository\\Generator\\GeneratorServiceProvider",
                "Shamaseen\\Repository\\Generator\\RepositoryServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Shamaseen\\Repository\\Generator\\": "src/"
        }
    },

    "minimum-stability": "stable",
    "prefer-stable": true,

    "require": {
        "illuminate/console": ">5.7",
        "illuminate/support": ">5.7",
        "doctrine/dbal": ">2.8",
        "illuminate/database": ">5.7",
        "illuminate/routing": ">5.7",
        "illuminate/validation": ">5.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "sebastian/phpcpd": "^4.1",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6"
    }
}
